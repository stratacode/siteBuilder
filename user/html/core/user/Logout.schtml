<%@ @URL(pattern="/logout", realTime=false) %>
<html extends="PageLayout" pageVisitCount="=: currentUserView.logout()">
<body>
   <div id="appFrame">
      <div id="pageBody">
         <div class="profileView" visible=":= !navbar.navMenu.loginVisible">
         You have signed out successfully.
         <a href="/">Continue as guest</a> or <a href="/login">Login</a>
         </div>
      </div>
   </div>
</body>
</html>
