<div visible=":= currentUserView.loginStatus==LoginStatus.NotLoggedIn">
   <div class="formError"><%= currentUserView.userViewError %></div>
   <input type="text" placeholder="user name" value=":=: currentUserView.userName">
   <input type="text" placeholder="password" value=":=: currentUserView.password">
   <input type="submit" value="Log in" clickEvent="=: currentUserView.login()">
   <div id="alt">
      <input type="submit" value="Log out" clickEvent="=: currentUserView.logout()">
   </div>
</div>
