<div class="AddressDisplayView">
   <%! Address address; %>
   <div><%= address.name %></div>
   <div><%= address.address1 %></div>
   <div visible=":= !TextUtil.isEmpty(address.address2)"><%= address.address2 %></div>
   <div><%= address.city %>, <%= address.state %> <%= address.postalCode %></div>
</div>
