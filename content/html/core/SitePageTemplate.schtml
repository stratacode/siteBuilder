<%@ @URL(pattern="/sites/{sitePathName=urlString}[/{pagePathName=urlString}]", testURLs={"/sites/stratacycles", "/sites/stratacycles/about"}) %>
<html extends="PageLayout">
   <%! String pagePathName; %>
   <body>
      <div id="appFrame">
         <div id="pageBody">
            Site: <%= siteView.siteContext.siteName %>
            Page: <%= pagePathName == null ? "home" : pagePathName %>
         </div>
      </div>
   </body>
</html>
